    <div class="center">

        <h4>
            <div class="top-8"><a href="#" id="theme-toggle" tabindex="3" data-i18n="theme_toggle">Switch theme</a>
            </div>
            <div class="top-8"><a href="https://bit.ly/vtxtools_info" tabindex="3" data-i18n="channel">Updates</a>
            </div>
        </h4>

        <script src="js/translations.js?v=1.0.6"></script>
        <script src="js/theme.js"></script>
        <script src="js/overlay.js"></script>
        <script src="js/expand.js"></script>


